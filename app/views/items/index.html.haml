  .main-contents
    .swiper-container
      .swiper-wrapper
        .swiper-slide
          = link_to do
            %img.banner-slider{ src: "https://www.mercari.com/jp/assets/img/common/jp/top/top-banner-kusanagi-xmas.jpg"}
        .swiper-slide
          = link_to do
            %img.banner-slider{ src: "https://www.mercari.com/jp/assets/img/common/jp/top/top-banner-new-register.png"}
        .swiper-slide
          = link_to do
            %img.banner-slider.welcome{ src: "https://www.mercari.com/jp/assets/img/common/jp/top/top-banner-welcome.png"}
      .swiper-pagination
      .swiper-button-prev
      .swiper-button-next

